{
  "name": "redkeepers-android-capacitor-wrapper",
  "version": "0.1.0",
  "private": true,
  "description": "Temporary Capacitor wrapper scaffold for RedKeepers Android vertical slice packaging.",
  "scripts": {
    "prepare:web": "python ../tools/android_capacitor_wrapper.py prepare --clean-web",
